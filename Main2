import java.lang.Math;
import java.util.Scanner;
public class Main2 {
    public static void main(String[] args) {
       double a, b, x, y;
       Scanner scan = new Scanner(System.in);
       System.out.println("напиши значение а");
       a = scan.nextDouble();
       System.out.println("напиши значение b");
       b = scan.nextDouble();
       System.out.println("напиши значение x");
       x = scan.nextDouble();
       y = Math.sqrt(a + b) < x ? (a * Math.pow(x, 2)) + (b * Math.log10(2 * x)) : Math.sqrt(a + Math.sin(2*x));
       System.out.println(y);
    }
}
